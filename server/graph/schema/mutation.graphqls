# GraphQL mutation definitions
# https://gqlgen.com/getting-started/

input NewUser {
  name: String!
}

input DeleteUser {
  id: ID!
}

input CreateProfile {
  name: String!
  birthDate: String!
  gender: String!
  height: Float!
  weight: Float!
}

type Mutation {
  deleteUser(input: DeleteUser!): Boolean!
  createProfile(input: CreateProfile!): Profile!
}