{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_SERVER_IP": "EXPO_PUBLIC_SERVER_IP",
        "EXPO_PUBLIC_SERVER_PORT": "EXPO_PUBLIC_SERVER_PORT"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SERVER_IP": "EXPO_PUBLIC_SERVER_IP",
        "EXPO_PUBLIC_SERVER_PORT": "EXPO_PUBLIC_SERVER_PORT"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_SERVER_IP": "EXPO_PUBLIC_SERVER_IP",
        "EXPO_PUBLIC_SERVER_PORT": "EXPO_PUBLIC_SERVER_PORT"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
